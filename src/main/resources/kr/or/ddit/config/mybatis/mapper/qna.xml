<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="qna">

	<select id="selectAllQna" resultType="qnaVo">
		SELECT
		    qna_no	
		    ,qna_parent_no
		    ,writer
		    ,title
		    ,content
		    ,reg_dt
		    ,hit
		    ,use_yn
		FROM
		    qna
	</select>
	

</mapper>