<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="items">
<!-- 품목 쿼리 -->

<!-- 분류코드로 품목(작물명)찾기 -->
<select id="selectItems" resultType="itemsVo" parameterType="int">
	select category_code,item_code,item_nm from items
	order by item_code
</select>	

</mapper>

